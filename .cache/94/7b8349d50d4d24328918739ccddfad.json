 white;\r\n        &--name{\r\n            width: 100%;\r\n            padding: 0 0.5rem;\r\n        }\r\n    }\r\n}","styles\\components\\_post.scss":"@use \"../var\";\r\n@use \"../mixin\";\r\n\r\n.post{\r\n    max-width: 95%;\r\n    margin: auto;\r\n    border: solid 2px black;\r\n    border-radius: 6px;\r\n    overflow: hidden;\r\n\r\n    &__profile{\r\n        margin: var.$default-padding;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        padding: 0.2em;\r\n        border: solid 1px black;\r\n        max-width: 100%;\r\n        &--name{\r\n            width: 100%;\r\n            padding: var.$default-padding;\r\n        }\r\n    }\r\n\r\n    &__buttons{\r\n        display: flex;\r\n        gap: 0.5em;\r\n        padding: 0 0.2em;\r\n    }\r\n\r\n    &__content{\r\n        padding: var.$default-padding;\r\n    }\r\n\r\n    &__tags{\r\n        padding: 0.5rem;\r\n        span{\r\n            padding: 0.2em 0.6em;\r\n            background-color: rgb(194 50 74);\r\n            border-radius: 6px;\r\n            margin: 0 0.1em;\r\n            color: white;\r\n            font-weight: 700;\r\n        }\r\n    }\r\n\r\n    &__footer{\r\n        display: flex;\r\n        justify-content: space-between;\r\n        padding: var.$default-padding;\r\n        background: var.$content-background-2;\r\n    }\r\n\r\n}","styles\\components\\_side.scss":"@use \"../var\";\r\n@use \"../mixin\";\r\n\r\n.side{\r\n    padding: var.$default-padding;\r\n    min-width: 15rem;\r\n    height: fit-content;\r\n    margin-left: 0.5em;\r\n    @include mixin.main-appearance;\r\n}\r\n","styles\\_mixin.scss":"@use \"var\";\r\n\r\n@mixin main-appearance{\r\n    background: var.$content-background;\r\n    border: solid 1px black;\r\n    border-radius: 2px;\r\n}","styles\\components\\_main-content.scss":"@use \"../var\";\r\n@use \"../mixin\";\r\n\r\n.main-content{\r\n    min-width: 15rem;\r\n    background: var.$content-background;\r\n    @include mixin.main-appearance;\r\n\r\n    &__title{\r\n        margin: 1rem 2rem;\r\n        font-size: 2rem;\r\n        font-weight: 800;\r\n    }\r\n}\r\n","styles\\components\\_button.scss":"@use \"../var\";\r\n@use \"../mixin\";\r\n\r\n.button{\r\n    padding: 0.4em;\r\n    border: solid 1px #0169a4;\r\n    border-radius: 3px;\r\n    background: linear-gradient(0deg, rgb(0 104 163), rgb(118 199 255));\r\n    transition: background-color 0.2s linear;\r\n    color: white;\r\n    box-shadow: 0px 1px 0 0px #00000059;\r\n    &:hover{\r\n        background: linear-gradient(0deg, rgb(0, 109, 172), rgb(128, 202, 255));\r\n        transition: background-color 0.2s linear;\r\n    }\r\n    &:active{\r\n        background: linear-gradient(0deg, rgb(0, 78, 122), rgb(11, 118, 194));\r\n        transition: background-color 0.2s linear;\r\n        padding-top: 0.5em;\r\n        padding-bottom: 0.3em;\r\n        box-shadow: 0px 1px 0 0px #00000059;\r\n    }\r\n}\r\n","styles\\components\\_new-post.scss":"@use \"../var\";\r\n@use \"../mixin\";\r\n\r\n.new-post{\r\n    padding: var.$default-padding;\r\n    label{\r\n        width: 100%;\r\n        display: block;\r\n    }\r\n    &__form{\r\n        display: flex;\r\n        gap: var.$default-padding;\r\n    }\r\n\r\n    &__imagearea{\r\n        width: 10rem;\r\n        background: #c4c4c4;\r\n        height: 10rem;\r\n        border: dashed 3px grey;\r\n        border-radius: 8px;\r\n    }\r\n    &__textarea {\r\n        width: 100%;\r\n        border: solid 1px black;\r\n        border-radius: 6px;\r\n        overflow: hidden;\r\n        display: flex;\r\n        flex-direction: column;\r\n        textarea{\r\n            width: 95%;\r\n            height: 6rem;\r\n            margin: auto;\r\n            resize: none;\r\n        }\r\n        &--footer{\r\n            padding: 0.5rem;\r\n            background: var.$content-background-2;\r\n            text-align: end;\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":n